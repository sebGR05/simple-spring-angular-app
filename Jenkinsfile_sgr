pipeline {
	agent {
		docker {
			image 'node:6-alpine'
			args '-p 3000:3000'
		}
	}
	stages {
		stage('Build') {
			steps {
				sh 'npm install'
			}
		}
	}
pipeline {
	agent {
		docker {
			image 'openjdk:8-jdk-alpine'
			args '-p 8081:8081'
		}
	}
	stages {
		stage('Build') {
			steps {
				sh 'npm install'
			}
		}
        stage('Test_sgr') {
			steps {
				java -version
			}
		}
	}
}