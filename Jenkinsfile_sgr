pipeline {
    agent {
        docker {
            image 'openjdk:8-jdk-alpine'
            args '-p 8085:8080'
        }
    }
    /*triggers {
        cron('0 * * ? * *')
    }*/
    stages {
        stage('check_java') {
            when {
                branch 'dev'
            }
            steps {
                echo 'Hello, JDK'
                sh 'java -version'
            }
        }
    }
}